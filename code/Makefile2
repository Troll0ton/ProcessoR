all: square

square: CPU.o lines_handle.o input_output.o stack.o
	g++ CPU.o lines_handle.o input_output.o stack.o

main.o: CPU.cpp
	g++ -c CPU.cpp

lines_handle.o: lines_handle.cpp
	g++ -c lines_handle.cpp

input_output.o: input_output.cpp
	g++ -c input_output.cpp

stack.o: stack.cpp
	g++ -c stack.cpp

clean:
	rm -rf *.o
